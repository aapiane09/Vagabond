
<div class="row">
  <div class="col m4 offset-m4 form z-depth-5">
    <h2 class="center">Log In</h2>
      <%= form_for @user, url: "/sessions", method: "post" do |f| %>
        <%= f.email_field :email, placeholder: "Email" %>
        <%= f.password_field :password_digest, placeholder: "Password" %>
        <div class="row">
          <div class="col m6 offset-m3">
            <%= f.submit "Log In", class: "waves-effect waves-light btn-large form-button" %>
          </div>
        </div>
      <% end %>
  </div>
</div>
